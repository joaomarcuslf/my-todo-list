<h2><%= @todo_list.title %></h2>

<ul class="todo_items">
  <% @todo_list.todo_items.each do |todo_item| %>
    <li id="<%= dom_id(todo_item) %>" >
      <%= todo_item.content %>
      <%= link_to "Edit item", edit_todo_list_todo_item_path(todo_item) %>
    </li>
  <% end %>
</ul>
<p>
  <%= link_to "New Todo item", new_todo_list_todo_item_path %>
</p>
